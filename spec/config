# vim:ft=Marchfile
module main

config {
    Log {
        @Default "info"
        Level string
    }

    @Default ":8080"
    Address string

    Database {
        @Default "pg://user:pass@127.0.0.1/users?sslmode=disabled"
        URL string
    }
    Workers {
        Queue {
            @Minimum "1"
            @Default "100"
            @Maximum "200"
            Size int
        }
    }
    UpdateInterval interval
}
